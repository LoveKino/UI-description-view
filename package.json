{
  "name": "ui-description-view",
  "version": "0.0.45",
  "description": "UI assertion, a simple UI DSL, used to describe the restraint of some UI elements, after description a UI element, we can try to search in the area of page to find some elements which conform to these descriptions.",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "mocha \"test/function/**/*.js\"",
    "unit-test": "deftest -s src -d test/unit/dest -t test/unit/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoveKino/UI-description-view.git"
  },
  "keywords": [
    "ui",
    "description",
    "view"
  ],
  "author": "ddchen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LoveKino/UI-description-view/issues"
  },
  "homepage": "https://github.com/LoveKino/UI-description-view#readme",
  "devDependencies": {
    "browser-js-env": "0.0.9",
    "defcomment": "^0.4.7",
    "es6-promisify": "^5.0.0",
    "mocha": "^3.2.0",
    "readme-builder": "0.0.12",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "basetype": "^0.1.4",
    "bolzano": "^0.1.1",
    "color-similarity": "0.0.1",
    "kabanery": "^0.2.4",
    "kabanery-select": "0.0.9",
    "leta-ui": "^0.1.25",
    "onecolor": "^3.0.4"
  },
  "readmeDocOptions": {
    "ignores": [
      "**/*/node_modules/**/*",
      "node_modules/**/*",
      "test/**/*",
      "example/**/*"
    ]
  }
}
